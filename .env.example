# Telegram Bot Token (get from @BotFather)
TELEGRAM_TOKEN=your_telegram_bot_token_here

# Admin User ID (for accessing admin commands like /token_usage)
ADMIN_USER_ID=your_telegram_user_id_here

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Google Custom Search API credentials (optional)
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_google_search_engine_id_here

# Daily usage limits (to control costs)
DAILY_SEARCH_LIMIT=50
DAILY_MEDIA_LIMIT=10
DAILY_IMAGE_GEN_LIMIT=3 

# OpenAI model configuration
OPENAI_MODEL_DEFAULT=gpt-4o-mini
OPENAI_MODEL_VISION=gpt-4o
OPENAI_MODEL_SUMMARY=gpt-4o
OPENAI_MODEL_ANALYSIS=gpt-4o-mini
OPENAI_MODEL_TRANSLATION=gpt-4o-mini

# OpenWeather API Key (optional, for weather function)
OPENWEATHER_API_KEY=your_openweather_api_key_here 